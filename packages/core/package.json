    "rsbuild-plugin-publint": "^0.3.2",
    "rsbuild-plugin-virtual-module": "0.3.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2",
    "vfile": "^6.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  }
}
