{
  "name": "@rspress/plugin-shiki",
  "version": "2.0.0-beta.0",
  "description": "A plugin for rspress to record the last updated time of the doc.",
  "bugs": "https://github.com/web-infra-dev/rspress/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-infra-dev/rspress.git",
    "directory": "packages/plugin-last-updated"
  },
  "license": "MIT",
  "sideEffects": [
    "*.css",
    "*.less",
    "*.sass",
    "*.scss"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./shiki.css": {
      "default": "./dist/shiki.css"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build -w",
    "reset": "rimraf ./**/node_modules"
  },
  "dependencies": {
    "@rspress/shared": "workspace:*",
    "@shikijs/rehype": "^3.2.2",
    "hast-util-from-html": "^2.0.3",
    "shiki": "^3.2.2",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.4",
    "@rsbuild/plugin-sass": "~1.3.1",
    "@rslib/core": "0.6.5",
    "@rspress/config": "workspace:*",
    "@types/hast": "^3.0.4",
    "@types/node": "^18.11.17",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "react": "^19.1.0",
    "rsbuild-plugin-publint": "^0.3.0",
    "typescript": "^5.8.2",
    "unified": "^11.0.5"
  },
  "engines": {
    "node": ">=14.17.6"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  }
}
