{
  "name": "rspress",
  "version": "2.0.0-alpha.7",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/rspress",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/index.js"
    },
    "./runtime": {
      "default": "./runtime.ts"
    },
    "./theme": {
      "default": "./theme.ts"
    },
    "./config": {
      "default": "./config.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "rspress": "./bin/rspress.js"
  },
  "files": [
    "bin",
    "dist",
    "runtime.ts",
    "theme.ts",
    "config.ts"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build -w",
    "reset": "rimraf ./**/node_modules"
  },
  "dependencies": {
    "@rsbuild/core": "1.2.19",
    "@rspress/core": "workspace:*",
    "@rspress/shared": "workspace:*",
    "cac": "^6.7.14",
    "chokidar": "^3.6.0",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.1",
    "@rslib/core": "0.5.5",
    "@types/jest": "~29.5.14",
    "@types/node": "^18.11.17",
    "@types/react": "^18.3.19",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  }
}
