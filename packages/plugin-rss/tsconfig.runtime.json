    "composite": true,
    "declaration": true,
    "noEmit": false,
    "module": "ESNext",
    "target": "ESNext",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "jsx": "react-jsx"
  },
  "include": ["static"]
}
