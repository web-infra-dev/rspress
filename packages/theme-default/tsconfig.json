{
  "extends": "@rspress/config/tsconfig",
  "compilerOptions": {
    "composite": true,
    "outDir": "dist",
    "declarationDir": "dist",
    "rootDir": "src",
    "baseUrl": ".",
    "noUnusedLocals": false,
    "paths": {
      "#theme/*": ["./src/*"]
    }
  },
  "include": ["src"],
  "exclude": ["./src/node/source-build-plugin.ts"],
  "references": [
    {
      "path": "../runtime"
    },
    {
      "path": "../shared"
    }
  ]
}
