{
  "name": "@rspress/theme-default",
  "version": "1.37.0",
  "description": "The Default Theme of Rspress Documentation Framework",
  "bugs": "https://github.com/web-infra-dev/rspress/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/rspress",
    "directory": "packages/theme-default"
  },
  "license": "MIT",
  "type": "module",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/bundle.d.ts",
  "main": "./dist/index.js",
  "imports": {
    "#theme/*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./dist/bundle.d.ts",
      "default": "./dist/index.js"
    },
    "./node/*": "./dist/node/*",
    "./package.json": "./package.json",
    "./bundle.css": "./dist/bundle.css"
  },
  "scripts": {
    "dev": "modern build -w",
    "build": "modern build",
    "reset": "rimraf ./**/node_modules",
    "test": "echo nothing"
  },
  "engines": {
    "node": ">=14.17.6"
  },
  "dependencies": {
    "@mdx-js/react": "2.3.0",
    "@rspress/runtime": "workspace:*",
    "@rspress/shared": "workspace:*",
    "body-scroll-lock": "4.0.0-beta.0",
    "copy-to-clipboard": "^3.3.3",
    "flexsearch": "0.7.43",
    "htmlparser2": "^9.1.0",
    "htmr": "^1.0.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^1.3.0",
    "react-syntax-highlighter": "^15.6.1",
    "react-transition-group": "4.4.5"
  },
  "devDependencies": {
    "@modern-js/plugin-tailwindcss": "2.62.0",
    "@modern-js/tsconfig": "2.62.0",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/hast": "3.0.4",
    "@types/jest": "~29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/mdast": "^4.0.4",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "gray-matter": "4.0.3",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.5.3",
    "vitest": "2.1.4"
  },
  "sideEffects": [
    "*.css",
    "*.less",
    "*.sass",
    "*.scss",
    "**/virtual-global-styles.js",
    "virtual-global-styles",
    "./src/styles/index.ts",
    "./dist/index.js"
  ],
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  }
}
