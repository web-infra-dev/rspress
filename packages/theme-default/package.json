{
  "name": "@rspress/theme-default",
  "version": "2.0.0-beta.3",
  "description": "The Default Theme of Rspress Documentation Framework",
  "bugs": "https://github.com/web-infra-dev/rspress/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-infra-dev/rspress.git",
    "directory": "packages/theme-default"
  },
  "license": "MIT",
  "sideEffects": [
    "*.css",
    "*.less",
    "*.sass",
    "*.scss",
    "**/virtual-global-styles.js",
    "virtual-global-styles",
    "./src/styles/index.ts",
    "./dist/index.js"
  ],
  "type": "module",
  "imports": {
    "#theme/*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./dist/bundle.d.ts",
      "default": "./dist/index.js"
    },
    "./node/*": "./dist/node/*",
    "./package.json": "./package.json",
    "./bundle.css": "./dist/bundle.css"
  },
  "main": "./dist/index.js",
  "types": "./dist/bundle.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "//why build twice?": ".module.scss.d.ts is generaterd in webpack loader at the first time and parallel with tsc, it is flasky",
    "build": "rslib build || rslib build",
    "dev": "rslib build -w",
    "reset": "rimraf ./**/node_modules"
  },
  "dependencies": {
    "@dr.pogodin/react-helmet": "2.0.4",
    "@mdx-js/react": "2.3.0",
    "@rspress/runtime": "workspace:*",
    "@rspress/shared": "workspace:*",
    "body-scroll-lock": "4.0.0-beta.0",
    "copy-to-clipboard": "^3.3.3",
    "flexsearch": "0.7.43",
    "github-slugger": "^2.0.0",
    "htmr": "^1.0.2",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-syntax-highlighter": "^15.6.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.7",
    "@rsbuild/plugin-react": "~1.1.1",
    "@rsbuild/plugin-sass": "~1.3.1",
    "@rsbuild/plugin-svgr": "^1.0.7",
    "@rsbuild/plugin-typed-css-modules": "~1.0.2",
    "@rslib/core": "0.6.8",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/hast": "^3.0.4",
    "@types/jest": "~29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/mdast": "^4.0.4",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@types/react-syntax-highlighter": "^15.5.13",
    "gray-matter": "4.0.3",
    "rsbuild-plugin-publint": "^0.3.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  }
}
