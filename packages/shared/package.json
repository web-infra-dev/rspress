{
  "name": "@rspress/shared",
  "version": "2.0.0-rc.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-infra-dev/rspress.git",
    "directory": "packages/shared"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
