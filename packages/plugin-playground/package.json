{
  "name": "@rspress/plugin-playground",
  "version": "1.31.1",
  "description": "A plugin for rspress to preview the code block in markdown/mdx file.",
  "bugs": "https://github.com/web-infra-dev/rspress/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/rspress",
    "directory": "packages/plugin-preview"
  },
  "license": "MIT",
  "scripts": {
    "dev": "modern build -w",
    "build": "modern build",
    "reset": "rimraf ./**/node_modules",
    "test": "vitest run --passWithNoTests"
  },
  "types": "./dist/cli/esm/index.d.ts",
  "main": "./dist/cli/cjs/index.js",
  "module": "./dist/cli/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/cli/esm/index.d.ts",
      "import": "./dist/cli/esm/index.js",
      "default": "./dist/cli/cjs/index.js"
    },
    "./web": {
      "types": "./dist/web/esm/index.d.ts",
      "import": "./dist/web/esm/index.js",
      "default": "./dist/web/cjs/index.js"
    }
  },
  "engines": {
    "node": ">=14.17.6"
  },
  "dependencies": {
    "@mdx-js/mdx": "2.3.0",
    "@monaco-editor/react": "~4.6.0",
    "@oxidation-compiler/napi": "^0.2.0",
    "@rspress/shared": "workspace:*",
    "remark-gfm": "3.0.1",
    "rspack-plugin-virtual-module": "0.1.13"
  },
  "devDependencies": {
    "@babel/types": "^7.25.6",
    "@types/babel__core": "^7.20.5",
    "@types/babel__standalone": "^7.1.7",
    "@types/babel__traverse": "^7.20.6",
    "@types/mdast": "^3.0.15",
    "@types/node": "^18.11.17",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "mdast-util-mdxjs-esm": "^1.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.8.1",
    "typescript": "^5.5.3",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.2"
  },
  "peerDependencies": {
    "@rspress/core": "workspace:^1.31.1",
    "react": ">=17.0.0",
    "react-router-dom": "^6.8.1"
  },
  "files": [
    "dist",
    "static"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  }
}
